@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;


 
    
 <section class="service-section">
     <img class="backgrund-lines-right" src="~/images/backgrund-lines-right.svg" alt=""> 
        <div class="container">
                <div class="section-title">@Model.Value("ourServicesSectionsTitle")
                <h2>@Model.Value("ourServicesSectionsHeading")</h2>
                </div>
              <div class="service-boxes">
                     <div class="service-box">
                         @Html.GetBlockListHtml(Model.OurServices)
                     </div>
                    </div>
                     <div class="service-box-link">
                         <a class="btn-dark" href="@Model.Value("ourServicesSectionsButtonUrl")">@Model.Value("ourServicesSectionsButtonText")<i class="fa-solid fa-arrow-up-right"></i></a>
                     </div>
               
         </div>
    </section>
